<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.calmdawnstudio.rentbung">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_foreground_rentbung"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_rentbung_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.calmdawnstudio.rentbung.MainActivity"
            android:label="@string/title_activit_main"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name="com.calmdawnstudio.rentbung.ManualActivity" />
        <activity android:name="com.calmdawnstudio.rentbung.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.calmdawnstudio.rentbung.Authorization" />
        <activity android:name="com.calmdawnstudio.rentbung.LicenseActivity" />
        <activity android:name="com.calmdawnstudio.rentbung.SignUpActivity" />
    </application>

</manifest>